#matrix
row=int(input())
col=int(input())
matrix=[]
for i in range(row):
    rows=[]
    for j in range(col):
        val=int(input())
        rows.append(val)
    matrix.append(rows)
print(len(rows))#finding the no.of rows
print(len(matrix[0]))#finding the no.of columns

