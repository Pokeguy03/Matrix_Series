matrix=[[1,2,3],
      [4,5,6],    
      [7,8,9]]
n=len(matrix)
for row in range(len(matrix)):
    for col in range(row,len(matrix)):
        matrix[row][col],matrix[col][row]=matrix[col][row],matrix[row][col]
for j in range(n):          
    for i in range(len(matrix)//2):   
        matrix[i][j], matrix[n-1-i][j] = matrix[n-1-i][j], matrix[i][j]
for i in matrix:
    print(i)

Output:
[3, 6, 9]
[2, 5, 8]
[1, 4, 7]
